{
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "0 23 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/cron",
            "destination": "/api/cron.js"
        },
        {
            "source": "/",
            "destination": "/api/go"
        },
        {
            "source": "/admin",
            "destination": "/public/admin.html"
        },
        {
            "source": "/admin.html",
            "destination": "/public/admin.html"
        },
        {
            "source": "/go",
            "destination": "/api/go"
        },
        {
            "source": "/apply",
            "destination": "/public/apply.html"
        }
    ]
}